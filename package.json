{
  "name": "nextjs-boilerplate",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "predev": "tailwindcss build styles/tailwind.css -o public/tailwind.output.css",
    "prebuild": "cross-env NODE_ENV=production yarn run predev",
    "build": "next build",
    "build:analyze": "cross-env ANALYZE=true next build",
    "start": "next start",
    "export:html": "next build && next export -o dist",
    "svgr": "svgr public/svg/ -d icons/ --icon",
    "debug": "NODE_OPTIONS='--inspect' next dev"
  },
  "postcss": {
    "plugins": {
      "postcss-easy-import": {},
      "postcss-import": {},
      "postcss-each": {},
      "tailwindcss": {},
      "postcss-nested": {},
      "postcss-flexbugs-fixes": {},
      "postcss-preset-env": {
        "autoprefixer": {
          "flexbox": "no-2009"
        }
      }
    }
  },
  "dependencies": {
    "@glidejs/glide": "^3.4.1",
    "@rehooks/local-storage": "^2.4.0",
    "@rehooks/window-scroll-position": "^1.0.1",
    "@vunamhung/arg.js": "^1.4.0",
    "ahooks": "2.4.0",
    "apollo-server-micro": "^2.16.1",
    "axios": "^0.19.2",
    "classnames": "^2.2.6",
    "countries-list": "^2.5.6",
    "gsap": "^3.4.2",
    "knex": "^0.21.2",
    "lazy": "^1.0.11",
    "micro-cors": "^0.1.1",
    "mysql": "^2.18.1",
    "next": "9.5.1",
    "next-auth": "^3.1.0",
    "next-compose-plugins": "^2.1.1",
    "next-fonts": "1.4.0",
    "next-optimized-images": "2.6.2",
    "prop-types": "^15.6.2",
    "ramda": "0.27.1",
    "rc-drawer": "^4.1.0",
    "react": "16.13.1",
    "react-anchor-link-smooth-scroll": "^1.0.12",
    "react-dom": "16.13.1",
    "react-gsap": "^2.2.1",
    "react-particles-js": "3.3.0",
    "react-reveal": "^1.2.2",
    "react-scrollspy": "^3.4.3",
    "react-spring": "^8.0.27",
    "react-stickynode": "^3.0.3",
    "react-sweet-state": "^2.3.1",
    "react-use-gesture": "^7.0.15",
    "styled-jsx-plugin-postcss": "^2.0.1"
  },
  "devDependencies": {
    "@next/bundle-analyzer": "9.5.1",
    "@svgr/cli": "^5.4.0",
    "@tailwindcss/custom-forms": "^0.2.1",
    "@typescript-eslint/eslint-plugin": "3.8.0",
    "@typescript-eslint/parser": "3.8.0",
    "@vunamhung/tailwind-config": "0.9.5",
    "babel-eslint": "^10.1.0",
    "cross-env": "^7.0.2",
    "eslint": "7.6.0",
    "eslint-config-react-app": "^5.2.1",
    "eslint-plugin-flowtype": "^5.2.0",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-jsx-a11y": "^6.3.1",
    "eslint-plugin-react": "^7.20.5",
    "eslint-plugin-react-hooks": "^4.0.8",
    "image-trace-loader": "^1.0.2",
    "imagemin-mozjpeg": "9.0.0",
    "imagemin-optipng": "8.0.0",
    "imagemin-svgo": "8.0.0",
    "lqip-loader": "^2.2.0",
    "postcss-each": "^0.10.0",
    "postcss-easy-import": "^3.0.0",
    "postcss-flexbugs-fixes": "^4.2.1",
    "postcss-import": "^12.0.1",
    "postcss-nested": "^4.2.3",
    "postcss-preset-env": "^6.7.0",
    "responsive-loader": "2.0.0",
    "svg-sprite-loader": "^5.0.0",
    "tailwindcss": "1.6.2",
    "webp-loader": "^0.6.0"
  },
  "bit": {
    "env": {},
    "componentsDefaultDirectory": "components/{name}",
    "packageManager": "yarn"
  }
}
