    "ramda": "^0.27.0",
    "rc-drawer": "^4.1.0",
    "react": "16.13.1",
    "react-anchor-link-smooth-scroll": "^1.0.12",
    "react-dom": "16.13.1",
    "react-particles-js": "^3.2.1",
    "react-reveal": "^1.2.2",
