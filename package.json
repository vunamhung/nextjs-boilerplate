{
  "name": "nextjs-boilerplate",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "predev": "tailwindcss build src/assets/styles/tailwind.css -o src/assets/styles/tailwind.output.css",
    "build": "next build",
    "build:analyze": "cross-env ANALYZE=true next build",
    "start": "next start",
    "export:html": "next build && next export -o dist",
    "svgr": "svgr src/assets/svg/ -d src/icons/ --icon",
    "debug": "NODE_OPTIONS='--inspect' next dev"
  },
  "postcss": {
    "plugins": {
      "postcss-easy-import": {},
      "postcss-import": {},
      "postcss-each": {},
      "tailwindcss": {},
      "postcss-nested": {},
      "postcss-hexrgba": {},
      "postcss-flexbugs-fixes": {},
      "postcss-preset-env": {
        "autoprefixer": {
          "flexbox": "no-2009"
        }
      }
    }
  },
  "dependencies": {
    "@glidejs/glide": "^3.4.1",
    "@rehooks/local-storage": "^2.4.0",
    "@rehooks/window-scroll-position": "^1.0.1",
    "@tailwindui/react": "0.1.1",
    "@vunamhung/arg.js": "^1.4.0",
    "ahooks": "2.6.0",
    "apollo-server-micro": "2.17.0",
    "axios": "0.20.0",
    "classnames": "^2.2.6",
    "countries-list": "^2.5.6",
    "date-fns": "2.16.1",
    "dequal": "^2.0.2",
    "formalizer": "^2.1.3",
    "gsap": "3.5.1",
    "is-mobile": "^2.2.2",
    "js-cookie": "^2.2.1",
    "knex": "0.21.5",
    "lazy": "^1.0.11",
    "micro-cors": "^0.1.1",
    "mysql": "^2.18.1",
    "next": "9.5.3",
    "next-auth": "^3.1.0",
    "next-compose-plugins": "^2.1.1",
    "next-fonts": "1.4.0",
    "next-optimized-images": "2.6.2",
    "next-pwa": "3.1.4",
    "next-seo": "^4.7.3",
    "nextjs-sitemap-generator": "^1.1.1",
    "prop-types": "^15.6.2",
    "ramda": "0.27.1",
    "rc-collapse": "^2.0.0",
    "rc-drawer": "^4.1.0",
    "react": "16.13.1",
    "react-anchor-link-smooth-scroll": "^1.0.12",
    "react-countup": "^4.3.3",
    "react-customizable-progressbar": "^1.0.1",
    "react-div-100vh": "^0.5.0",
    "react-dom": "16.13.1",
    "react-gsap": "^2.2.1",
    "react-headroom": "^3.0.0",
    "react-hook-form": "^6.8.2",
    "react-intersection-observer": "8.28.5",
    "react-lazyload": "^3.0.0",
    "react-particles-js": "3.3.0",
    "react-photoswipe": "^1.3.0",
    "react-reveal": "^1.2.2",
    "react-scrollspy": "^3.4.3",
    "react-spring": "^8.0.27",
    "react-sweet-state": "^2.3.1",
    "react-table": "^7.5.0",
    "react-use-gesture": "7.0.16",
    "styled-jsx-plugin-postcss": "3.0.0",
    "use-google-spreadsheet": "^0.3.2",
    "use-places-autocomplete": "1.5.0",
    "validator": "^13.1.1",
    "vanilla-lazyload": "17.1.2"
  },
  "devDependencies": {
    "@next/bundle-analyzer": "9.5.3",
    "@svgr/cli": "^5.4.0",
    "@tailwindcss/custom-forms": "^0.2.1",
    "@tailwindcss/typography": "^0.2.0",
    "@typescript-eslint/eslint-plugin": "4.1.0",
    "@typescript-eslint/parser": "4.1.0",
    "babel-eslint": "^10.1.0",
    "cross-env": "^7.0.2",
    "eslint": "7.9.0",
    "eslint-config-react-app": "^5.2.1",
    "eslint-plugin-flowtype": "^5.2.0",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-jsx-a11y": "^6.3.1",
    "eslint-plugin-react": "7.20.6",
    "eslint-plugin-react-hooks": "4.1.2",
    "gray-matter": "^4.0.2",
    "husky": "4.3.0",
    "image-trace-loader": "^1.0.2",
    "imagemin-mozjpeg": "9.0.0",
    "imagemin-optipng": "8.0.0",
    "imagemin-svgo": "8.0.0",
    "lqip-loader": "2.2.1",
    "postcss-each": "^0.10.0",
    "postcss-easy-import": "^3.0.0",
    "postcss-flexbugs-fixes": "^4.2.1",
    "postcss-hexrgba": "^2.0.1",
    "postcss-import": "^12.0.1",
    "postcss-nested": "^4.2.3",
    "postcss-preset-env": "^6.7.0",
    "prettier": "^2.1.1",
    "pretty-quick": "3.0.2",
    "react-markdown": "^4.3.1",
    "responsive-loader": "2.1.1",
    "svg-sprite-loader": "^5.0.0",
    "tailwindcss": "1.8.9",
    "tailwindcss-aspect-ratio": "^3.0.0",
    "webp-loader": "^0.6.0"
  },
  "bit": {
    "env": {},
    "componentsDefaultDirectory": "src/components/{name}",
    "packageManager": "yarn",
    "resolveModules": {
      "aliases": {
        "@": "src"
      }
    }
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged"
    }
  }
}
